<html>

<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
    <link href="/public/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="fontawesome/css/all.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==" crossorigin="anonymous" referrerpolicy="no-referrer"
    />
    <script src="index.js"></script>
    <title>Personal Web</title>
</head>

<body>
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container-fluid">
            <a class="navbar-brand me-5 ms-5" href="/blog"><img src="/public/assets/logo.png" /></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active me-3" aria-current="page" href="/blog">Home</a
              >
            </li>
            <li class="nav-item">
              <a
                class="nav-link active text-orange"
                aria-current="page"
                href="/"
                >Add My Project</a
              >
            </li>
          </ul>
          <a href="/Contact-me" class="btn btn-black me-5">Contact Me</a>
            </div>
        </div>
    </nav>
    <!-- End Navbar -->

    <!-- Form -->
    <div class="container-form-blog d-flex justify-content-center align-items-center">
        <form action="/blog", method="post" onsubmit="addBlog(event)">
            <div class="h2 text-center">Add My Project</div>

            <div class="mb-3">
                <label for="input-title" class="form-label">Project Name</label>
                <input type="text" class="form-control" id="input-title" placeholder="" name="Project Name"/>
            </div>
            <div class="mb-3 d-flex justify-content-between">
                <div class="mb-3">
                    <label for="input-title" class="form-label">Start Date</label>
                    <input type="date" class="form-control" id="input-date-start" placeholder="" name="Start Date" />
                </div>

                <div class="mb-3">
                    <label for="input-title" class="form-label">End Date</label>
                    <input type="date" class="form-control" id="input-date-end" placeholder="" name="End Date"/>
                </div>
            </div>

            <div class="mb-3">
                <label for="input-content" class="form-label">Description</label>
                <textarea class="form-control" id="input-content" rows="5" placeholder="" name="Description"></textarea>
            </div>

            <div class="">
                <label for="input-checkbox" class="form-label">Technologies</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" value="" name="node js" id="flexCheckDefault1">
                <label class="form-check-label" for="flexCheckDefault">
                  Node Js
                </label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" value="" name="next js" id="flexCheckDefault2">
                <label class="form-check-label" for="flexCheckDefault">
                  Next Js
                </label>
            </div>
            <div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" value="" name="react js" id="flexCheckDefault3">
                    <label class="form-check-label" for="flexCheckDefault">
                  React Js
                </label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" value="" name="typescript" id="flexCheckDefault4">
                    <label class="form-check-label" for="flexCheckDefault">
                  TypeScript
                </label>
                </div>
            </div>

            <div class="mb-3">
                <label for="choose-file" class="form-label">Upload image</label>
                <input class="form-control" type="file" id="choose-file" name="upload image" />
            </div>

            <div class="d-flex justify-content-end mt-5">
                <button class="btn btn-orange" type="submit" onclick="addBlog()" src="/public/assets/file.png" alt="file">Submit</button>
            </div>
        </form>
    </div>
    <!-- End Form -->
    <script src="blog.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>

</html>